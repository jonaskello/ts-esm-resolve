{
  "name": "ts-esm-resolve",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "license": "MIT",
  "description": "Resolve typescript ES modules",
  "dependencies": {
    "json5": "^2.1.2",
    "strip-bom": "^3.0.0",
    "arg": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "esbuild": "^0.13.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "tsc && cp -a ./src/support ./lib/support",
    "rebuild": "yarn clean && yarn build",
    "clean": "rm -rf lib",
    "try": "yarn build && cd ./examples/example1 && TS_NODE_PROJECT=./packages/server/tsconfig.json node --loader ../../lib/loader/myloader.js --experimental-specifier-resolution=node --enable-source-maps ./packages/server/src/server.ts"
  }
}
